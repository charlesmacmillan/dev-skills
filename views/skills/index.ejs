<%- include('../partials/head') %>
  <body>
    <img src="https://i.imgur.com/43aWexW.png" alt="mandog" />
    <main>
      <h1>My Skills</h1>
      <nav> <a href="/">Home</a> | <a href="/skills/new">Add New Skill </a></nav>
      <p>Click on a skill</p>
      <ul>
        <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%= t.id %>"><%= t.skill %></a>
        </li>
        <% }); %>
      </ul>

    </main>
  </body>
</html>
